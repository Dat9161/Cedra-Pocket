{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SKIP_ENV_VALIDATION": "true"
  },
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci --ignore-scripts"
}